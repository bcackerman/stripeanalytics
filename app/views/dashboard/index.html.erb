<div class="container-narrow">
	<%= render 'layouts/partials/app-header' %>

	<!-- overview title/header -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box-title">
				<strong class="title-fix">Overview</strong>
				<div class="pull-right">
					<form class="form-horizontal date-picker">
					  <fieldset>
					    <div class="control-group">
					      <div class="controls">
					        <i class="icon-calendar"></i>
					        <%= text_field_tag 'revenue-dates', "#{(Date.today - 30.days).strftime("%m/%d/%Y")} - #{Date.today.strftime("%m/%d/%Y")}" %>
					      </div>
					    </div>
					  </fieldset>
					</form>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>	

	<!-- overview charts -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box no-spacing">
				<ul id="overview-squares">
					<li>
						<div class="inner">
							<h4>Churn Rate</h4>
							<h3><%= number_to_percentage(@churn_rate, :precision => 1) %></h3>
						</div>
					</li>
					<li>
						<div class="inner">
							<h4>Revenue / Customer</h4>
							<h3>$2,342.34</h3>
						</div>
					</li>
					<li>
						<div class="inner">
							<h4>Trial to Paid</h4>
							<h3>75.6%</h3>
						</div>
					</li>
					<li>
						<div class="inner">
							<h4>Monthly Revenue <span class="green">+12%</span></h4>
							<h3>$10,436.34 </h3>
						</div>
					</li>
				</ul>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>

	<!-- revenue chart title/header -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box-title">
				<strong class="title-fix">Revenue</strong>
				<div class="pull-right">
					<%= link_to "<i class='icon-signal'></i> Predict".html_safe, "#{}", :class => "btn" %>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>	

	<!-- review charts -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box">
				CHART
				<div class="clearfix"></div>
			</div>
		</div>
	</div>

	<!-- valuable customers chart title/header -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box-title">
				<strong>Most Valuable Customers</strong>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>	

	<!-- valuable customers -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box">
				<table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Username</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>1</td>
              <td>Mark</td>
              <td>Otto</td>
              <td>@mdo</td>
            </tr>
            <tr>
              <td>2</td>
              <td>Jacob</td>
              <td>Thornton</td>
              <td>@fat</td>
            </tr>
            <tr>
              <td>3</td>
              <td>Larry</td>
              <td>the Bird</td>
              <td>@twitter</td>
            </tr>
          </tbody>
        </table>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>

</div>