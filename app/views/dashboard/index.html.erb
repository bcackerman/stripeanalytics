<div class="container-narrow">
	<%= render 'layouts/partials/app-header' %>

	<!-- overview title/header -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box-title">
				<strong>Overview</strong>
				<div class="pull-right">
					<form class="form-horizontal date-picker">
					  <fieldset>
					    <div class="control-group">
					      <div class="controls">
					          <i class="icon-calendar"></i>
					          <%= text_field_tag 'revenue-dates', "#{(Date.today - 30.days).strftime("%m/%d/%Y")} - #{Date.today.strftime("%m/%d/%Y")}" %>
					      </div>
					    </div>
					  </fieldset>
					</form>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>	

	<!-- overview charts -->
	<div class="row-fluid">
		<div class="span12">
			<div class="box">

			</div>
		</div>
	</div>

</div>